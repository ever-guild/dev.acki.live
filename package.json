    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "sass-embedded": "^1.93.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "type": "module"
}
