{
  "name": "acki-live-svelte",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "vite dev",
    "dev": "vite dev",
    "release:gh-pages": "gh-pages --nojekyll --cname acki.live --repo git@github.com:ever-guild/acki.live.git --dotfiles --branch release --dist build --message 'release'",
    "release": "npm run build && npm run release:gh-pages",
    "build:after": "cp build/index.html build/404.html && cp -r etc/.github build",
    "build": "vite build && npm run build:after",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "serve": "http-server build -p 8080 -o",
    "serve:prod": "NODE_ENV='production' npm run build && npm run serve",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tvmsdk/lib-node": "https://bafybeibmyfx4vbbvjvydk4w52gb6dp4qc3722ojhqogvoetr5nvpw5z2ou.ipfs.w3s.link/",
    "@types/node": "^24.8.1",
    "autoprefixer": "^10.4.16",
    "gh-pages": "^6.3.0",
    "http-server": "^14.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass-embedded": "^1.93.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "type": "module",
  "dependencies": {
    "@tvmsdk/core": "^2.1.1",
    "@tvmsdk/lib-web": "^2.0.1"
  }
}
