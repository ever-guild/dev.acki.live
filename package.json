{
  "name": "acki-live",
  "version": "1.0.0",
  "scripts": {
    "release:gh-pages": "gh-pages --no-history --nojekyll --cname acki.live --repo git@github.com:ever-guild/acki.live.git --branch release --dist dist/acki-live",
    "release": "npm run build:production && npm run release:gh-pages",
    "ng": "ng",
    "start": "ng serve",
    "build:production:after": "cp src/assets/attest-release-artifacts.yml dist/acki-live/.github/workflows/",
    "build:production": "ng build --configuration production --base-href / && npm run build:production:after",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.6",
    "@angular/common": "^20.3.6",
    "@angular/compiler": "^20.3.6",
    "@angular/core": "^20.3.6",
    "@angular/forms": "^20.3.6",
    "@angular/platform-browser": "^20.3.6",
    "@angular/platform-browser-dynamic": "^20.3.6",
    "@angular/router": "^20.3.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.6",
    "@angular/cli": "^20.3.6",
    "@angular/compiler-cli": "^20.3.6",
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.16",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.8.0"
  }
}
